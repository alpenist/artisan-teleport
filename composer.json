{
    "name": "alpenist/artisan-teleport",
    "description": "",
    "keywords": [
        "alpenist",
        "artisan-teleport"
    ],
    "homepage": "https://github.com/alpenist/artisan-teleport",
    "license": "MIT",
    "authors": [
        {
            "name": "Hiro Garabedian",
            "email": "h.g@signsandcolors.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Ait\\ArtisanTeleport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ait\\ArtisanTeleport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ait\\ArtisanTeleport\\ArtisanTeleportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
